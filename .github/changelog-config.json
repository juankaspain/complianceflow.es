{
  "categories": [
    {
      "title": "ğŸš€ Features",
      "labels": ["feat", "feature", "enhancement"]
    },
    {
      "title": "ğŸ› Bug Fixes",
      "labels": ["fix", "bug", "bugfix"]
    },
    {
      "title": "ğŸ“ Documentation",
      "labels": ["docs", "documentation"]
    },
    {
      "title": "ğŸ› ï¸ Maintenance",
      "labels": ["chore", "refactor", "style"]
    },
    {
      "title": "âš¡ Performance",
      "labels": ["perf", "performance"]
    },
    {
      "title": "ğŸ”’ Security",
      "labels": ["security"]
    },
    {
      "title": "ğŸ§ª Tests",
      "labels": ["test", "tests"]
    },
    {
      "title": "ğŸ”§ CI/CD",
      "labels": ["ci", "cd", "build"]
    },
    {
      "title": "ğŸ“¦ Dependencies",
      "labels": ["dependencies", "deps"]
    }
  ],
  "ignore_labels": [
    "skip-changelog",
    "duplicate",
    "invalid",
    "wontfix"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}\n\n### Contributors\n\n${{CONTRIBUTORS}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "^(feat|feature)(\\(.+\\))?:",
      "target": "feat"
    },
    {
      "pattern": "^fix(\\(.+\\))?:",
      "target": "fix"
    },
    {
      "pattern": "^docs?(\\(.+\\))?:",
      "target": "docs"
    },
    {
      "pattern": "^(chore|refactor|style)(\\(.+\\))?:",
      "target": "chore"
    },
    {
      "pattern": "^perf(\\(.+\\))?:",
      "target": "perf"
    },
    {
      "pattern": "^test(\\(.+\\))?:",
      "target": "test"
    },
    {
      "pattern": "^(ci|build)(\\(.+\\))?:",
      "target": "ci"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[.*\\]",
    "on_property": "title"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\] )?",
      "target": ""
    }
  ],
  "max_tags_to_fetch": 200,
  "max_pull_requests": 200,
  "max_back_track_time_days": 365,
  "exclude_merge_branches": []
}
